pipeline {

  agent any
  stages {
    stage ('clone') {
     steps {
       git url: 'https://github.com/Geethanjali2905/jenkins-demo.git'
        branch: 'main'
      }
     }
     stage('Run script')
     {
       steps {
         sh 'chmod +x script.sh'
         sh './script.sh'
        }
     }
   }
}
